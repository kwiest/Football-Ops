<h1>All Divisions</h1>

<% if admin? %>
	<p><%= button_to "New division", new_division_path, :method => :get %></p>
<% end %>

<ul id="divisions">
	<% @divisions.each do |d| %>
		<li>
			<%= link_to d.name, d %>
			<% if admin? %>
				| <%= link_to "(edit)", edit_division_path(d) %>
			<% end %>
		</li>
	<% end %>
</ul>