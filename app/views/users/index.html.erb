<h1>All DFOs</h1>

<%= render 'search_users' %>

<% if admin? %>
	<p><%= button_to "New user", new_user_path, :method => :get %></p>
<% end %>

<%= will_paginate @users %>

<table>
	<tr>
		<th>Name</th>
		<th>School</th>
		<th>Contact Info</th>
		<th>Conference Rep</th>
		<th>National Committee</th>
		<th>Affiliation</th>
		<% if admin? %>
			<th>Edit</th>
			<th>Delete</th>
		<% end %>
	</tr>
	<%= render @users %>
</table>

<%= will_paginate @users %>