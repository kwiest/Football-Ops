<% div_for @school do %>
	<%= f.error_messages %>
	
	<% f.inputs do %>
		<%= f.input :conference %>
		<%= f.input :district %>
		<%= f.input :division %>
	<% end %>
	
	<% f.inputs do %>
		<%= f.input :name %>
		<%= f.input :address1 %>
		<%= f.input :city %>
		<%= f.input :state %>
		<%= f.input :zip %>
	<% end %>
	
	<% f.buttons do %>
		<%= f.commit_button "Save school" %> or <%= link_to 'Cancel', schools_path, :class => :cancel %>
	<% end %>
<% end %>