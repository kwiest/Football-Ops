<fieldset>
  <%= f.input :name, placeholder: "Western State University" %>
  <%= f.input :address1, label: "Street address", placeholder: "123 A St" %>
  <%= f.input :city, placeholder: "Anywhere" %>
  <%= f.input :state, placeholder: "CA" %>
  <%= f.input :zip, placeholder: "12345" %>
</fieldset>

<fieldset>
  <%= f.association :conference, required: true, include_blank: false %>
  <%= f.association :district, required: true, include_blank: false %>
  <%= f.association :division, required: true, include_blank: false %>
</fieldset>